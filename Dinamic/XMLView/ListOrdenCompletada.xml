<view>
    <columns>
        <column name="company" order="100">
            <widget type="select" fieldname="idempresa" onclick="EditEmpresa">
                <values source="empresas" fieldcode="idempresa" fieldtitle="nombre"/>
            </widget>
        </column>
        <column name="project" order="110">
            <widget type="select" fieldname="idproyecto" onclick="EditProyecto">
                <values source="proyectos" fieldcode="idproyecto" fieldtitle="nombre"/>
            </widget>
        </column>
        <column name="task" order="120">
            <widget type="select" fieldname="idtarea" onclick="EditTarea">
                <values source="tareas" fieldcode="idtarea" fieldtitle="nombre"/>
            </widget>
        </column>
        <column name="process" order="130">
            <widget type="select" fieldname="idproceso" onclick="EditProceso">
                <values source="procesos" fieldcode="idproceso" fieldtitle="nombre"/>
            </widget>
        </column>
        <column name="amount-processed" order="140">
            <widget type="integer" fieldname="cantidadprocesada"/>
        </column>
        <column name="date" title="processed-date" display="right" order="150">
            <widget type="date" fieldname="fechaprocesada"/>
        </column>
        <column name="hour" title="processed-hour" order="160">
            <widget type="time" fieldname="horaprocesada"/>
        </column>
        <column name="precio" title="price-hour" display="right" order="170">
            <widget type="money" fieldname="precio"/>
        </column>
        <column name="user" order="180">
            <widget type="select" fieldname="nick" onclick="EditUser">
                <values source="users" fieldcode="nick" fieldtitle="nick"/>
            </widget>
        </column>
    </columns>
</view>
